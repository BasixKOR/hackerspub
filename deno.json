    "i18next": "npm:i18next@^24.0.5",
    "ioredis": "npm:ioredis@^5.4.1",
    "json-canonicalize": "npm:json-canonicalize@^1.0.6",
    "katex": "npm:katex@^0.16.11",
    "keyv": "npm:keyv@^5.2.1",
    "keyv-file": "npm:keyv-file@^5.0.3",
    "markdown-it": "npm:markdown-it@^14.1.0",
    "markdown-it-abbr": "npm:markdown-it-abbr@^2.0.0",
    "markdown-it-anchor": "npm:markdown-it-anchor@^9.2.0",
    "markdown-it-deflist": "npm:markdown-it-deflist@^3.0.0",
    "markdown-it-footnote": "npm:markdown-it-footnote@^4.0.0",
    "markdown-it-github-alerts": "npm:markdown-it-github-alerts@^0.3.1",
    "markdown-it-graphviz": "npm:markdown-it-graphviz@^1.1.2",
    "markdown-it-texmath": "npm:markdown-it-texmath@^1.0.0",
    "markdown-it-toc-done-right": "npm:markdown-it-toc-done-right@^4.2.0",
    "postgres": "npm:postgres@^3.4.5",
    "preact": "npm:preact@^10.25.2",
    "@preact/signals": "npm:@preact/signals@^1.3.1",
    "satori": "npm:satori@^0.11.3",
    "satori-html": "npm:satori-html@^0.3.2",
    "sharp": "npm:sharp@^0.33.5",
    "tailwindcss": "npm:tailwindcss@3.4.3",
    "tailwindcss/plugin": "npm:tailwindcss@3.4.3/plugin.js",
    "tinyld": "npm:tinyld@^1.3.4",
    "tinyld.browser": "https://cdn.jsdelivr.net/npm/tinyld@1.3.0/dist/tinyld.normal.browser.js",
    "xss": "npm:xss@^1.0.15"
  },
  "unstable": [
    "kv",
    "temporal"
  ],
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.asynciterable",
      "dom.iterable",
      "deno.ns",
      "deno.unstable"
    ],
    "jsx": "precompile",
    "jsxImportSource": "preact",
    "jsxPrecompileSkipElements": [
      "a",
      "img",
      "source",
      "body",
      "html",
      "head"
    ]
  },
  "nodeModulesDir": "auto"
}
