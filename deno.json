{
  "name": "@hongminhee/hackerspub",
  "version": "0.1.0",
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check .",
    "test": "deno test --env-file=.env.test --allow-env --allow-net --parallel",
    "dev": "deno run -A --watch=static/,routes/ dev.ts",
    "build": "deno run -A dev.ts build",
    "start": "deno run -A main.ts",
    "update": "deno run -A -r jsr:@fresh/update .",
    "drizzle-kit": "deno run -A npm:drizzle-kit",
    "migrate": "deno run -A npm:drizzle-kit migrate",
    "migrate:generate": "deno run -A npm:drizzle-kit generate",
    "migrate:test": "deno run --env-file=.env.test -A npm:drizzle-kit migrate",
    "keygen": "deno run --allow-read --allow-env scripts/keygen.ts",
    "hooks:install": "deno run --allow-read=deno.json,.git/hooks/ --allow-write=.git/hooks/ jsr:@hongminhee/deno-task-hooks",
    "hooks:pre-commit": "deno task check"
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "jsr",
        "recommended"
      ],
      "exclude": [
        "no-slow-types",
        "react-no-danger"
      ]
    }
  },
  "fmt": {
    "exclude": [
      "*.md"
    ]
  },
  "exclude": [
    "**/_fresh/*",
    ".github/",
    "drizzle/",
    "node_modules/",
    "static/"
  ],
  "exports": {
    ".": "./main.ts"
  },
  "imports": {
    "@aws-sdk/client-s3": "npm:@aws-sdk/client-s3@^3.750.0",
    "@aws-sdk/s3-request-presigner": "npm:@aws-sdk/s3-request-presigner@^3.750.0",
    "@fedify/fedify": "jsr:@fedify/fedify@1.5.0-dev.717+38e2fb51",
    "@fedify/markdown-it-mention": "jsr:@fedify/markdown-it-mention@^0.2.0",
    "@fedify/postgres": "jsr:@fedify/postgres@^0.2.2",
    "@fedify/redis": "jsr:@fedify/redis@^0.3.0",
    "@hongminhee/x-forwarded-fetch": "jsr:@hongminhee/x-forwarded-fetch@^0.2.0",
    "@keyv/redis": "npm:@keyv/redis@^4.0.2",
    "@logtape/logtape": "jsr:@logtape/logtape@^0.8.2",
    "@logtape/sentry": "jsr:@logtape/sentry@^0.1.0",
    "@mdit-vue/plugin-title": "npm:@mdit-vue/plugin-title@^2.1.3",
    "@onisaint/validate-email": "jsr:@onisaint/validate-email@^1.0.1",
    "@opentelemetry/api": "npm:@opentelemetry/api@^1.9.0",
    "@opentelemetry/context-async-hooks": "npm:@opentelemetry/context-async-hooks@^1.28.0",
    "@opentelemetry/instrumentation": "npm:@opentelemetry/instrumentation@^0.55.0",
    "@opentelemetry/instrumentation-ioredis": "npm:@opentelemetry/instrumentation-ioredis@^0.45.0",
    "@opentelemetry/instrumentation-redis-4": "npm:@opentelemetry/instrumentation-redis-4@^0.44.0",
    "@opentelemetry/resources": "npm:@opentelemetry/resources@^1.28.0",
    "@opentelemetry/sdk-trace-base": "npm:@opentelemetry/sdk-trace-base@^1.28.0",
    "@opentelemetry/semantic-conventions": "npm:@opentelemetry/semantic-conventions@^1.28.0",
    "@resvg/resvg-wasm": "npm:@resvg/resvg-wasm@^2.6.2",
    "@schotsl/mailgun": "jsr:@schotsl/mailgun@^1.3.0",
    "@searking/markdown-it-cjk-breaks": "npm:@searking/markdown-it-cjk-breaks@^2.0.1-0",
    "@sentry/core": "npm:@sentry/core@^8.39.0",
    "@sentry/deno": "npm:@sentry/deno@^8.38.0",
    "@sentry/opentelemetry": "npm:@sentry/opentelemetry@^8.39.0",
    "@shikijs/markdown-it": "npm:@shikijs/markdown-it@^1.22.2",
    "@shikijs/transformers": "npm:@shikijs/transformers@^1.22.2",
    "@std/assert": "jsr:@std/assert@^1.0.7",
    "@std/collections": "jsr:@std/collections@^1.0.9",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.2",
    "@std/encoding": "jsr:@std/encoding@^1.0.6",
    "@std/html": "jsr:@std/html@^1.0.3",
    "@std/http": "jsr:@std/http@^1.0.9",
    "@std/path": "jsr:@std/path@^1.0.8",
    "@std/text": "jsr:@std/text@^1.0.8",
    "@std/uuid": "jsr:@std/uuid@^1.0.4",
    "@tailwindcss/typography": "npm:@tailwindcss/typography@^0.5.15",
    "@valibot/valibot": "jsr:@valibot/valibot@^0.42.1",
    "arcsecond": "npm:arcsecond@^5.0.0",
    "cssfilter": "npm:cssfilter@^0.0.11",
    "drizzle-kit": "npm:drizzle-kit@^0.28.0",
    "drizzle-orm": "npm:drizzle-orm@^0.36.3",
    "flydrive": "npm:flydrive@^1.1.0",
    "fresh": "jsr:@fresh/core@^2.0.0-alpha.29",
